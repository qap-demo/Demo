{
  "version": 3,
  "name": "API-EventFinder",
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/krakend/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/api-eventfinder/events/{eventId}",
      "method": "GET",
      "output_encoding": "no-op",
      "input_headers": [
        "*"
      ],
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "name": "{eventId}",
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false,
          "debug": false,
          "url_pattern": "/v1/events/{eventId}",
          "encoding": "no-op",
          "sd": "static",
          "timeout": "3000ms",
          "cache_ttl": "300s",
          "disable_host_sanitize": false
        }
      },
      "backend": [
        {
          "url_pattern": "/v1/events/{eventId}",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "extra_config": {},
          "host": [
            "https://api.eventfinder.com"
          ],
          "cache_ttl": "300s"
        }
      ]
    },
