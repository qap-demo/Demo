{
  "version": 3,
  "timeout": "5000ms",
  "cache_ttl": "300s",
  "output_encoding": "no-op",
  "port": 3890,
  "plugin": {
    "pattern": ".so",
    "folder": "/etc/plugins/"
  },
  "endpoints": [
    {
      "endpoint": "/api-pokemon/pokemon/ditto",
      "method": "GET",
      "output_encoding": "no-op",
      "input_headers": [
        "*"
      ],
      "input_query_strings": [],
      "extra_config": {
        "security/cors": {
          "name": "ditto",
          "allow_origins": [
            "*"
          ],
          "allow_methods": [
            "GET",
            "HEAD",
            "POST"
          ],
          "expose_headers": [
            "Content-Length",
            "Content-Type"
          ],
          "allow_headers": [
            "Accept-Language"
          ],
          "max_age": "12h",
          "allow_credentials": false,
          "debug": false,
          "url_pattern": "/api/v2/pokemon/ditto",
          "encoding": "no-op",
          "sd": "static",
          "timeout": "3000ms",
          "cache_ttl": "300s",
          "host": [
            "https://pokeapi.co"
          ],
          "disable_host_sanitize": false
        }
      },
      "backend": [
        {
          "url_pattern": "/api/v2/pokemon/ditto",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "extra_config": {},
          "host": [
            "https://pokeapi.co"
          ],
          "cache_ttl": "300s"
        }
      ]
    }
  ],
  "extra_config": {
    "plugin/http-server": {
      "name": [
        "qap-server-billing"
      ],
      "qap-server-billing": {
        "endpoint_checker": "http://qap-demo.qriarlabs.cloud:8091/check",
        "endpoint_charge": "http://qap-demo.qriarlabs.cloud:8092/charge"
      }
    }
  }
}